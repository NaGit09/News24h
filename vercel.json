{
  "rewrites": [
    {
      "source": "/tts",
      "destination": "/api/tts"
    },
    {
      "source": "/api/rss/:match*",
      "destination": "https://cdn.24h.com.vn/upload/rss/:match*"
    },
    {
      "source": "/api/:match*",
      "destination": "https://www.24h.com.vn/:match*"
    },
    {
      "source": "/tts",
      "destination": "https://api.murf.ai/v1/speech/generate"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
